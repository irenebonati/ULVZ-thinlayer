\rm *.jwh
gfortran Superbee_Implicit_Diff.f
./a.out
psxy h00000.jwh -R0/1/0/22 -Jx8/1 -Ba0.2/a2 -W1p -V -K > o.ps
ls -1 *.jwh > filelist.jwh
numfiles=`wc filelist.jwh | awk '{print $1}'`
numfiles=`expr $numfiles + 0`
count=1
while [ $count -lt $numfiles ]
do
  count=`expr $count + 1`
  infile=`head -$count filelist.jwh | tail -1`
  psxy $infile -R0/1/0/22 -Jx8/1 -Ba0.2/a2 -W1p -V -O -K  >> o.ps
done
psxy $infile -R0/1/0/22 -Jx8/1 -Ba0.2/a2 -W1p -V -O >> o.ps
\rm filelist.jwh
